{
    "env": {
        "browser": true,
        "es6": true,
        "node" : true,
        "jquery" : true
    },
    "extends": "eslint:recommended",
    "rules": {
        "indent": [
            "error",
            4
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-console":0
    },
    "globals": {
        "d3": false,
        "global": false
    }
}